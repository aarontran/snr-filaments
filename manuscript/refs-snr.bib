	Arxivurl = {http://arXiv.org/abs/1312.3367},
	Author = {{Winkler}, P.~F. and {Williams}, B.~J. and {Reynolds}, S.~P. and {Petre}, R. and {Long}, K.~S. and {Katsuda}, S. and {Hwang}, U.},
	Date-Added = {2014-09-04 16:29:00 +0000},
