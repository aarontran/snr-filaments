	Doi = {10.1093/mnras/stt100},
	Eprint = {1301.3173},
	Journal = {\mnras},
	Keywords = {acceleration of particles, instabilities, plasmas, cosmic rays},
	Month = apr,
	Pages = {2873-2884},
	Primaryclass = {astro-ph.HE},
	Title = {{Universal behaviour of shock precursors in the presence of efficient cosmic ray acceleration}},
	Volume = 430,
	Year = 2013,
