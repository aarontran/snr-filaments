	Author = {{Hayato}, A. and {Yamaguchi}, H. and {Tamagawa}, T. and {Katsuda}, S. and {Hwang}, U. and {Hughes}, J.~P. and {Ozawa}, M. and {Bamba}, A. and {Kinugasa}, K. and {Terada}, Y. and {Furuzawa}, A. and {Kunieda}, H. and {Makishima}, K.},
	Date-Added = {2014-09-07 07:11:42 +0000},
	Date-Modified = {2014-09-07 07:11:43 +0000},
	Doi = {10.1088/0004-637X/725/1/894},
