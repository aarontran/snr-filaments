	Date-Modified = {2014-09-02 21:17:53 +0000},
	Doi = {10.1088/0004-637X/703/1/662},
	Eprint = {0907.4756},
	Journal = {\apj},
	Keywords = {galaxies: jets, radiation mechanisms: non-thermal, supernova remnants, supernovae: individual: B0540-693, X-rays: ISM},
	Month = sep,
	Pages = {662-670},
	Primaryclass = {astro-ph.HE},
	Title = {{Synchrotron-Loss Spectral Breaks in Pulsar-Wind Nebulae and Extragalactic Jets}},
	Volume = 703,
