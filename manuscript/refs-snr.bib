	Doi = {10.1051/0004-6361:20040130},
	Eprint = {astro-ph/0404203},
	Journal = {\aap},
	Keywords = {cosmic rays, shock acceleration, nonthermal emission, supernova remnants, individual: Cas A},
	Month = may,
	Pages = {L27-L30},
	Title = {{Direct evidence of efficient cosmic ray acceleration and magnetic field amplification in Cassiopeia A}},
	Volume = 419,
	Year = 2004,
	Bdsk-Url-1 = {http://dx.doi.org/10.1051/0004-6361:20040130},
