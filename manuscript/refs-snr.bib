	Date-Added = {2014-09-07 06:00:09 +0000},
	Date-Modified = {2014-09-07 06:00:09 +0000},
	Doi = {10.1126/science.1194293},
	Eprint = {1010.4306},
	Journal = {Science},
	Month = oct,
	Pages = {347-},
	Primaryclass = {astro-ph.CO},
	Title = {{Particle Acceleration on Megaparsec Scales in a Merging Galaxy Cluster}},
	Volume = 330,
	Year = 2010,
