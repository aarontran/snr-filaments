	Title = {{A Spatial and Spectral Study of Nonthermal Filaments in Historical Supernova Remnants: Observational Results with Chandra}},
	Volume = 621,
	Year = 2005,
	Bdsk-Url-1 = {http://arXiv.org/abs/astro-ph/0411326},
